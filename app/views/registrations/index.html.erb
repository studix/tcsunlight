<h1>Latest registrations</h1>

<table class="table table-striped">
    <thead>
  <tr>
    <th>Name</th>
    <th>Vorname</th>
    <th>Email</th>
    <th>Telefonnummer</th>
    <th>Klassierung</th>
    <th>Klassierungswert</th>
    <th>Herren Einzel</th>
    <th>Einzel Senioren</th>
    <th>Herren Doppel</th>
    <th>Partner HE Doppel</th>
    <th>Team-Cup</th>
    <th>Partner Team-Cup</th>
    <th>Bemerkung</th>
    <th>Tournament</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>
</thead>
<% @registrations.each do |registration| %>
  <tr>
    <td><%= registration.Name %></td>
    <td><%= registration.Vorname %></td>
    <td><%= registration.Email %></td>
    <td><%= registration.TelefonNummer %></td>
    <td><%= registration.Klassierung %></td>
    <td><%= registration.Klassierungswert %></td>
    <td><%= registration.draw_men_singles %></td>
    <td><%= registration.draw_men_seniors %></td>
    <td><%= registration.draw_men_doubles %></td>
    <td><%= registration.partner_men_doubles %></td>
    <td><%= registration.draw_team_cup %></td>
    <td><%= registration.partner_team_cup %></td>
    <td><%= registration.comment %></td>
    <td><%= registration.tournament_id %></td>
    <td><%= link_to 'Show', registration %></td>
    <td><%= link_to 'Edit', edit_registration_path(registration) %></td>
    <td><%= link_to 'Destroy', registration, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />